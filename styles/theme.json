{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"title": "FSE-Child by MB",
	"settings": {
		"layout": {
			"contentSize": "960px",
			"wideSize": "1200px"
		},
		"color": {
			"palette": [
				{
					"color": "#ffffff",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "#445058",
					"name": "Contrast",
					"slug": "contrast"
				},
				{
					"color": "transparent",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#445058",
					"name": "Secondary",
					"slug": "secondary"
				},
				{
					"color": "#008939",
					"name": "Tertiary",
					"slug": "tertiary"
				},
				{
					"color": "#26a9b2",
					"name": "Quaternary",
					"slug": "quaternary"
				}
			]
		},
		"typography": {
			"dropCap": false,
			"fluid": true,
			"fontFamilies": [
				{
				"fontFace": [
					{
						"fontFamily": "Calibri",
						"fontStretch": "normal",
						"fontStyle": "normal",
						"fontWeight": "400",
						"src": [
							"file:./assets/fonts/Calibri/Calibri.ttf"
						]
					},
					{
						"fontFamily": "Calibri-Italic",
						"fontStretch": "normal",
						"fontStyle": "italic",
						"fontWeight": "400",
						"src": [
							"file:./assets/fonts/Calibri/Calibri-Italic.ttf"
						]
					},
					{
						"fontFamily": "Calibri-Bold",
						"fontStretch": "normal",
						"fontStyle": "normal",
						"fontWeight": "700",
						"src": [
							"file:./assets/fonts/Calibri/Calibri-Bold.ttf"
						]
					}
				],
				"fontFamily": "\"Calibri\", sans-serif",
				"name": "Calibri",
				"slug": "calibri"
				}
			]
		}
	},
	"styles": {
		"elements": {
			"button": {
				"border": {
					"radius": "5px"
				}
			},
			"heading": {
				"typography": {
					"fontFamily":"var(--wp--preset--font-family--calibri)"
				}
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--calibri)",
			"lineHeight": "1.5"
		}
	}
}
